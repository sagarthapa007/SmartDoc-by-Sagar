    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.5",
    "react-router-dom": "^6.26.2",
    "react-window": "^1.8.10",
    "recharts": "^2.15.4",
    "simple-statistics": "^7.8.8",
